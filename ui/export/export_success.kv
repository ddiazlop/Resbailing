<ExportSuccessScreen>
    RelativeLayout:
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: 'ui/media/images/tobofondo2_estrellas.png'
    GridLayout:
        cols: 1
        rows: 3
        padding: 10
        spacing: 10
        size_hint_y: None
        pos_hint: {'center_x': .5, 'center_y': .5}
        height: self.minimum_height
        Image:
            source: 'ui/media/images/toboicono.png'
            size_hint_y: None
            height: 100
        Label:
            text: 'Exportación completada con éxito'
            font_size: 30
            color: 0, 0, 0, 1
            size_hint_y: None
            height: 100
        Button:
            text: 'Volver'
            size_hint_y: None
            height: 100
            on_press: root.change_screen('Main')